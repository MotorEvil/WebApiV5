<div class="row">
    <div id="map"></div>
    <script>
        var map;
        function initMap() {
            map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: 54.897887, lng: 23.90114 },
                zoom: 14
            });
            marker = new google.maps.Marker({
                position: {
                    lat: 54.897887,
                    lng: 23.90114
                },
                map: map,
                draggable: false
            });
        }

    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCHTGQC6zn76MukAHnpuwcMlbxpzVGWqz0&callback=initMap"
            async defer></script>
